{
  "name": "nabais.me",
  "version": "2023.12.1",
  "private": true,
  "description": "nabais.me website",
  "author": "Luis Nabais <luis@nabais.me>",
  "license": "MIT",
  "main": "src/pages/index.jsx",
  "engines": {
    "node": ">= 20.0.0",
    "npm": ">= 9.0.0"
  },
  "scripts": {
    "prepare": "npm run astro:sync",
    "astro:sync": "ASTRO_TELEMETRY_DISABLED=1 astro sync",
    "build": "npm run build:web",
    "build:web": "ASTRO_TELEMETRY_DISABLED=1 NODE_ENV=production astro build",
    "clean": "rimraf .astro coverage dist node_modules/.vite src/**/*.generated.*",
    "debug": "npm run debug:serve",
    "debug:serve": "NODE_ENV=development astro dev",
    "debug:test": "vitest dev",
    "serve": "ASTRO_TELEMETRY_DISABLED=1 astro preview",
    "lint": "run-s lint:types lint:js lint:styles",
    "lint:fix": "run-s lint:fix:js lint:fix:styles",
    "lint:types": "ASTRO_TELEMETRY_DISABLED=1 astro check && tsc --noEmit",
    "lint:fix:js": "eslint --fix .",
    "lint:js": "eslint .",
    "lint:styles": "stylelint src/**/*.sass src/**/*.scss src/**/*.css",
    "lint:fix:styles": "stylelint src/**/*.sass src/**/*.scss src/**/*.css --fix",
    "start": "npm run debug",
    "test": "npm run test:js",
    "test:js": "vitest --run --passWithNoTests",
    "test:js:update": "vitest --run -u --passWithNoTests",
    "test:ci": "CI=true vitest --coverage --passWithNoTests"
  },
  "browserslist": [
    "> 0.2%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "dependencies": {
    "@fontsource/play": "^5.0.18",
    "@fontsource/roboto-condensed": "^5.0.14",
    "@icons-pack/react-simple-icons": "^9.3.0",
    "@rooks/use-key": "^4.11.2",
    "@sentry/browser": "^7.100.1",
    "clsx": "^2.1.0",
    "feather-icons": "^4.29.1",
    "iso-url": "^1.2.1",
    "keen-slider": "^6.8.6",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "preact": "^10.19.4",
    "prop-types": "^15.8.1",
    "react-feather": "^2.0.10",
    "react-image-lightbox": "^5.1.4"
  },
  "overrides": {
    "react": "npm:@preact/compat@latest",
    "react-dom": "npm:@preact/compat@latest"
  },
  "devDependencies": {
    "@astrojs/check": "^0.5.2",
    "@astrojs/preact": "^3.1.0",
    "@astrojs/prefetch": "^0.4.1",
    "@astrojs/ts-plugin": "^1.5.0",
    "@sentry/cli": "^2.28.0",
    "@types/lodash-es": "^4.17.12",
    "@types/prop-types": "^15.7.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-istanbul": "^1.2.2",
    "astro": "^4.3.5",
    "astro-eslint-parser": "^0.16.3",
    "eslint": "^8.56.0",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "sass": "^1.70.0",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^5.2.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}
