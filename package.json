{
  "name": "nabais.me",
  "version": "2022.8.2",
  "private": true,
  "description": "nabais.me website",
  "author": "Luis Nabais <luis@nabais.me>",
  "license": "MIT",
  "main": "src/pages/index.jsx",
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 8.0.0"
  },
  "scripts": {
    "build": "npm run build:web",
    "build:web": "NODE_ENV=production astro build",
    "clean": "rimraf coverage dist node_modules/.vite src/**/*.generated.*",
    "debug": "npm run debug:serve",
    "debug:serve": "NODE_ENV=development astro dev",
    "debug:test": "BABEL_ENV=test jest --watch --maxWorkers=2",
    "serve": "astro preview",
    "lint": "run-s lint:types lint:js lint:styles",
    "lint:fix": "run-s lint:fix:js lint:fix:styles",
    "lint:types": "tsc --noEmit",
    "lint:fix:js": "eslint --fix .",
    "lint:js": "eslint .",
    "lint:styles": "stylelint src/**/*.sass src/**/*.scss src/**/*.css",
    "lint:fix:styles": "stylelint src/**/*.sass src/**/*.scss src/**/*.css --fix",
    "start": "npm run debug",
    "test": "npm run test:js",
    "test:js": "NODE_ENV=test jest --passWithNoTests ${JEST_OPTS}",
    "test:js:update": "NODE_ENV=test jest -u",
    "test:ci": "NODE_ENV=test jest --ci --passWithNoTests --coverage"
  },
  "browserslist": [
    "> 0.2%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "dependencies": {
    "@astrojs/preact": "^2.2.1",
    "@astrojs/prefetch": "^0.3.0",
    "@fontsource/play": "^5.0.5",
    "@fontsource/roboto-condensed": "^5.0.5",
    "@icons-pack/react-simple-icons": "^8.0.1",
    "@rooks/use-key": "^4.11.2",
    "@sentry/browser": "^7.60.0",
    "astro": "^2.9.4",
    "clsx": "^2.0.0",
    "feather-icons": "^4.29.0",
    "iso-url": "^1.2.1",
    "keen-slider": "^6.8.6",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "preact": "^10.16.0",
    "react-feather": "^2.0.10",
    "react-image-lightbox": "^5.1.4"
  },
  "overrides": {
    "react": "npm:@preact/compat@latest",
    "react-dom": "npm:@preact/compat@latest"
  },
  "devDependencies": {
    "@sentry/cli": "^2.19.4",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "astro-eslint-parser": "^0.14.0",
    "dotenv-flow": "^3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^4.1.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.45.0",
    "eslint-plugin-astro": "^0.27.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "sass": "^1.64.1",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
